<% if user_signed_in? %>
 <div class="row">
  <h1>
    <%= personal_address %>'s messages:
  </h1>
    <div class="col-md-8">
      <% if @messages.present? %>
        <h3> Messages (<%= @messages.count %>)</h3>
        <ol class="messages">
          <%= render @messages %>
        </ol>
          <%= will_paginate @messages %>
      <% end %>
    </div>
  </div>
<% end %>
